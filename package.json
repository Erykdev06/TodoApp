{
  "name": "todolistapp",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint --ext .tsx --ext .tsx ./src",
    "lint:fix": "eslint --fix --ext .tsx --ext .tsx ./src",
    "format": "prettier --check ./src",
    "write": "prettier --write ./src",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@expo/webpack-config": "^18.0.1",
    "@shopify/restyle": "^2.4.2",
    "@types/react": "~18.2.7",
    "expo": "~48.0.17",
    "expo-status-bar": "~1.4.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.71.8",
    "react-native-web": "~0.18.10",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@types/react-native": "^0.72.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8"
  },
  "private": true,
  "lint-staged": {
    "*.ts": [
      "yarn run lint",
      "yarn run format",
      "git add ."
    ]
  }
}
